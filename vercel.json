{
  "version": 2,
  "builds": [
    { "src": "api/nodecode.js", "use": "@vercel/node" },
    { "src": "public/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/nodecode.js" },
    {
      "handle": "filesystem"
    },
    { "src": "/(.*)", "dest": "/public/index.html" }
  ]
}
